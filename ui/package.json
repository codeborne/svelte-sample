{
  "engines": {
    "node": "~18"
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build && cp -r i18n build/i18n",
    "preview": "vite preview",
    "check": "svelte-check --diagnostic-sources js,svelte --compiler-warnings a11y-missing-attribute:ignore,a11y-label-has-associated-control:ignore,a11y-click-events-have-key-events:ignore,security-anchor-rel-noreferrer:ignore,a11y-autofocus:ignore,a11y-no-static-element-interactions:ignore",
    "check:dev": "npm run check -- --watch",
    "test": "vitest run",
    "test:dev": "vitest watch"
  },
  "browserslist": [
    "> 0.5%",
    "not dead"
  ],
  "dependencies": {
    "@codeborne/i18n-json": "^0.2.1",
    "svelte": "^4.0.1",
    "svelte-navigator": "^3.2.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/svelte": "^4.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "chai": "^4.3.6",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "svelte-check": "^3.4.4",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
